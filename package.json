{
  "name": "ncsa-mosaic-by-chromium",
  "version": "1.0.0",
  "description": "NCSA Mosaic style browser using Electron.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=x64 --out=dist --icon=icons",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=dist --icon=icons",
    "package-all": "electron-packager . --overwrite --all --out=dist"
  },
  "keywords": [
    "electron",
    "webview",
    "retro"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron": "^27.1.3"
  },
  "devDependencies": {
    "electron-packager": "^17.1.2"
  }
}